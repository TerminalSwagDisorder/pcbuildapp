</template>
<template name="postSubmit">
 
<table>
<tr> 
{{#each cases}}
   <td>{{ext525b}}</td>
 <td>{{id}}</td>
 <td>{{int35b}}</td>
 <td>{{name}}</td>
<td>{{price}}</td>
<td>{{ratings}}</td>
<td>{{type}}</td>
 {{/each}}
</tr>
</table> 

</template>


<template name="orderList">
  <!-- order table -->
  <div class="hpanel hgreen">
    <div class="panel-heading hbuilt">
     
    </div>
    <div class="panel-body">
      <div class="table-collapse">
        <table class="table table-condensed table-bordered">
          <thead>
          <tr>
            <th>{{mf 'Ext525b' 'Total'}}</th>
            <th>{{mf 'Id' 'Items'}}</th>
            <th>{{mf 'Int35b' 'int'}}</th>
            <th>{{mf 'Name' 'name'}}</th>
            <th>{{mf 'Price' 'price'}}</th>
            <th>{{mf 'Popularity' 'ratings'}}</th>
            <th>{{mf 'Type' 'type'}}</th>
          </tr>
          </thead>
          <tbody>
          {{#each sortedOrders}}
            {{> orderItem}}
          {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</template>

<template name="orderItem">
  <tr class="{{rowStyle status}}">
<td data-label="Total">{{ext525b}}</td>
    <td data-label="Items">{{id}}</td>
    <td data-label="int">{{int35b}}</td>
 <td data-label="name">{{name}}</td>
<td data-label="price">{{price}}</td>
<td data-label="ratings">{{ratings}}</td>
<td data-label="type">{{type}}</td>

  </tr>
</template>

